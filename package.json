{
  "name": "priority-q",
  "version": "3.0.1",
  "description": "A simple modern JavaScript implementation of a priority queue data structure class.",
  "main": "index.js",
  "keywords": [
    "priority queue",
    "priority-queue",
    "priorityQueue",
    "priority",
    "queue",
    "heap"
  ],
  "homepage": "https://github.com/raymond-lam/priority-q",
  "scripts": {
    "clean": "rimraf -rf coverage",
    "test": "eslint --ignore-path .npmignore . && cross-env NODE_ENV=test nyc --reporter=lcov --reporter=text mocha"
  },
  "author": {
    "name": "Raymond Lam",
    "email": "ray@lam-ray.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/raymond-lam/priority-q.git"
  },
  "license": "MIT",
  "nyc": {
    "require": [
      "babel-register"
    ],
    "sourceMap": false,
    "instrument": false,
    "lines": 100,
    "statements": 100,
    "functions": 100,
    "branches": 100,
    "check-coverage": true,
    "temp-directory": "./coverage/.nyc_output"
  },
  "babel": {
    "env": {
      "test": {
        "plugins": [
          "istanbul"
        ]
      }
    }
  },
  "devDependencies": {
    "babel-plugin-istanbul": "^4.1.6",
    "babel-register": "^6.26.0",
    "chai": "^4.1.2",
    "cross-env": "^5.2.0",
    "eslint": "^5.0.1",
    "mocha": "^5.2.0",
    "nyc": "^12.0.2",
    "rimraf": "^2.6.2"
  },
  "dependencies": {}
}
